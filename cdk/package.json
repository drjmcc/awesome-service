{
  "name": "awesome-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc && cp -r package.json test/test-config.json dist/",
    "watch": "tsc -w",
    "test": "yarn run build && jest",
    "lint": "eslint --max-warnings 0 '*/**/*.ts' --ignore-pattern '*/**/*.d.ts' --ignore-pattern 'cdk.out/**/*'",
    "lint.fix": "eslint --fix '*/**/*.ts' --ignore-pattern '*/**/*.d.ts' --ignore-pattern 'cdk.out/**/*'",
    "cdk": "cdk",
    "deploy.dev": "yarn run cdk deploy DEVAwesomeServiceComputeStack",
    "build.synth.run.local": "yarn run build && yarn run cdk synth DEVAwesomeServiceComputeStack && yarn run local.start-api",
    "local.start-api": "sam local start-api --template ./cdk.out/DEVAwesomeServiceComputeStack.template.json"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.66",
    "@types/config": "0.0.36",
